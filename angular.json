{
  "version": 1,
  "projects": {
    "eichfx": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/eichfx",
      "sourceRoot": "apps/eichfx/src",
      "prefix": "eichfx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/eichfx",
            "index": "apps/eichfx/src/index.html",
            "main": "apps/eichfx/src/main.ts",
            "polyfills": "apps/eichfx/src/polyfills.ts",
            "tsConfig": "apps/eichfx/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/eichfx/src/favicon.ico", "apps/eichfx/src/assets"],
            "styles": ["apps/eichfx/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/eichfx/src/environments/environment.ts",
                  "with": "apps/eichfx/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "eichfx:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "eichfx:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "eichfx:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/eichfx/tsconfig.app.json",
              "apps/eichfx/tsconfig.spec.json",
              "apps/eichfx/tsconfig.editor.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/eichfx/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/eichfx/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "eichfx-e2e": {
      "root": "apps/eichfx-e2e",
      "sourceRoot": "apps/eichfx-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/eichfx-e2e/cypress.json",
            "tsConfig": "apps/eichfx-e2e/tsconfig.e2e.json",
            "devServerTarget": "eichfx:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "eichfx:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/eichfx-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/eichfx-e2e/**/*"]
          }
        }
      }
    },
    "ui": {
      "projectType": "library",
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "prefix": "eichfx",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/ui/tsconfig.lib.json",
              "libs/ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/ui/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {}
    },
    "landing-page": {
      "projectType": "library",
      "root": "libs/landing-page",
      "sourceRoot": "libs/landing-page/src",
      "prefix": "eichfx",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/landing-page/tsconfig.lib.json",
              "libs/landing-page/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/landing-page/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/landing-page/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "schematics": {}
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "eichfx"
}
